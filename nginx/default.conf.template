error_log  /logs/error.log error;
access_log /logs/access.log;

server {
    listen      ${NGINX_PORT};
    server_name _;

    location ${NGINX_PATH} {
        proxy_set_header   X-Real-IP $remote_addr;
        proxy_set_header   Host      $http_host;
        proxy_pass         http://web:8080/;
    }
}